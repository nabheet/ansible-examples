alias deploy_local_haproxy="chmod -v 0600 /vagrant/insecure_private_key && time ansible-playbook -i /vagrant/lamp_haproxy_hosts /vagrant/lamp_haproxy/site.yml -u vagrant --private-key=/vagrant/insecure_private_key"
alias rolling_update_haproxy="chmod -v 0600 /vagrant/insecure_private_key && time ansible-playbook -i /vagrant/lamp_haproxy_hosts /vagrant/lamp_haproxy/rolling_update.yml -u vagrant --private-key=/vagrant/insecure_private_key"
export ANSIBLE_HOST_KEY_CHECKING=False
